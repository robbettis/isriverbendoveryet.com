{
  "name": "isriverbendoveryet.com",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "date-fns": "^2.14.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.1.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.0.10",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "react-scripts": "3.4.3",
    "source-map-explorer": "^2.4.2",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "lint": "eslint --fix 'src/**/*.{ts,tsx}'",
    "favicon": "convert public/images/icons/favicon-{16,32,48,64}.png public/favicon.ico"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "quotes": [
        "error",
        "double"
      ],
      "semi": "error"
    }
  },
  "browserslist": {
    "production": [
      "defaults",
      "not dead",
      "not IE 11",
      "not IE_Mob 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageReporters": [
      "text",
      "html"
    ]
  }
}
